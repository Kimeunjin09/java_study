
public class Ex2_1 {

	public static void main(String[] args) {
		
		// print() : 출력 후에 줄바꿈x
		// println() : 출력 후에 줄바꿈o
		// ""안은 문자로 취급
		
		System.out.println("Hello"); // printlin은 줄바꿈
		System.out.print("Hello"); 
		System.out.println("Hello");
		System.out.println(3+5);
		System.out.println("3+5");
		
		
	}

}
